{
  "name": "sunglasses",
  "version": "0.0.0",
  "description": "Sunglasses web app",
  "main": "sunglasses-pwa.js",
  "author": "Amir Najafi <njfamirm@gmail.com>",
  "private": "true",
  "contributors": [
    "Amir Najafi <njfamirm@gmail.com>"
  ],
  "license": "MIT",
  "scripts": {
    "b": "npm-run-all clean build:debug",
    "bp": "npm-run-all clean build:production",
    "build:debug": "export DEBUG_MODE=true && node esbuild.config.js",
    "build:production": "export DEBUG_MODE=false && node esbuild.config.js",
    "c": "yarn clean",
    "clean": "rm -rf static/*.{js,css,map}",
    "l": "yarn lint",
    "f": "npm-run-all --sequential format lint",
    "lint": "eslint **/*.ts --fix",
    "format": "prettier \"**/*.{html,css,json,md,ts}\" --ignore-path ./.eslintignore --write"
  },
  "dependencies": {
    "@alwatr/logger": "^0.11.0",
    "@alwatr/signal": "^0.11.0",
    "dom-to-image": "^2.6.0",
    "esbuild": "^0.14.38",
    "file-saver": "^2.0.5",
    "lit": "^2.0.2",
    "prettier": "^2.6.2"
  },
  "devDependencies": {
    "@types/dom-to-image": "^2.6.4",
    "@types/file-saver": "^2.0.5",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-google": "^0.14.0",
    "eslint-import-resolver-typescript": "^2.7.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-wc": "^1.3.2",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.5.4"
  }
}
